type Todo {
  id: ID!
  title: String!
  description: String!
  completed: Boolean!
  user: User!
}

type Query {
  todos: [Todo!]!
}

input NewTodo {
  title: String!
  description: String!
  completed: Boolean!
}

input CompletedstatusInput {
  id: ID!
  completed: Boolean!
}

type Mutation {
  createTodo(input: NewTodo!): Todo!
  changeCompletedStatus(input: CompletedstatusInput!): Todo!
  deleteTodo(id: ID!): [Todo!]!
}
